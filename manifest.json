{
  "manifest_version": 3,
  "name": "USF Tool",
  "description": "Base Level Extension",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/shopify.js"
      ],
      "matches": [
        "https://*.com/*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ]
}